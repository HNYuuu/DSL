[
    {
        "scope": "func",
        "watch": [
            "a"
        ],
        "assume": "a >= 1"
    },
    {
        "scope": ".*func.*",
        "regex": true,
        "watch": [
            "b"
        ],
        "ensure": "b < 100"
    },
    {
        "scope": "func",
        "nesting": [
            0
        ],
        "watch": [
            "var0",
            "var1"
        ],
        "require": "var0 >= var1"
    },
    {
        "scope": "func",
        "watch": [
            "iterates"
        ],
        "require": "iter(iterates) != 5"
    },
    {
        "scope": "may-vul",
        "check": [
            "integer-overflow",
            "divzero"
        ]
    },
    {
        "scope": "may-vul",
        "watch_op": "add",
        "watch": [
            "var1",
            "var2"
        ],
        "require": "var1 + var2 >= var1 and var1 + var2 >= var2"
    }
]